<div id="<%= dom_id chat %>">
  <p>
    <strong>Sender:</strong>
    <%= chat.sender_id %>
  </p>

  <p>
    <strong>Receiver:</strong>
    <%= chat.receiver_id %>
  </p>

  <p>
    <strong>Closed by sender:</strong>
    <%= chat.closed_by_sender %>
  </p>

  <p>
    <strong>Closed by receiver:</strong>
    <%= chat.closed_by_receiver %>
  </p>

</div>
